{
  "name": "dev-team-fall-25",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "dashboard": "bun run --filter '@dev-team-fall-25/server' dashboard",
    "clean": "turbo run clean && rm -rf node_modules",
    "check": "turbo run check",
    "check:types": "turbo run check:types"
