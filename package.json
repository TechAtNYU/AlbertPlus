    "turbo": "^2.5.8",
    "@types/bun": "1.3.0"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  },
  "trustedDependencies": [
    "@clerk/shared",
    "@tailwindcss/oxide",
    "esbuild",
    "sharp",
    "workerd"
  ],
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@stepperize/react": "^5.1.9",
    "cmdk": "^1.1.1",
    "react-hook-form": "^7.65.0",
    "zod": "^4.1.12"
  }
