{
  "name": "browser",
  "displayName": "Browser",
  "version": "0.0.1",
  "description": "A course registration companion for Albert",
  "author": "Tech@NYU <hello@techatnyu.org>",
  "scripts": {
    "dev": "doppler run -- plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package",
    "check": "biome check",
    "check:types": "tsc -noEmit"
  },
  "dependencies": {
    "@clerk/chrome-extension": "^2.7.7",
    "convex": "^1.28.0",
    "plasmo": "0.90.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.4.1"
  },
  "devDependencies": {
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "postcss": "8.4.33",
    "typescript": "5.3.3"
  },
  "manifest": {
    "key": "$CRX_PUBLIC_KEY",
    "permissions": [
      "cookies",
      "storage"
    ],
    "host_permissions": [
      "https://*/*",
      "http://localhost/*",
      "$CLERK_FRONTEND_API/*"
    ]
  }
}
